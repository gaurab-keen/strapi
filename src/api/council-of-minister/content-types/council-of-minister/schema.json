{
  "kind": "collectionType",
  "collectionName": "council_of_ministers",
  "info": {
    "singularName": "council-of-minister",
    "pluralName": "council-of-ministers",
    "displayName": "Council of Ministers",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Profile_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Facebook_URL": {
      "type": "string"
    },
    "Twitter_URL": {
      "type": "string"
    },
    "Contact_URL": {
      "type": "string"
    },
    "Minister_type": {
      "type": "enumeration",
      "enum": [
        "Prime Minister",
        "Cabinet Minister",
        "Ministers of State (Independent Charge)",
        "State Minister"
      ],
      "required": true
    },
    "Weight": {
      "type": "integer",
      "required": true,
      "max": 60,
      "min": 1,
      "default": 1
    },
    "Language": {
      "type": "enumeration",
      "enum": [
        "Language neutral",
        "English"
      ],
      "default": "English"
    },
    "Body": {
      "type": "text"
    },
    "review": {
      "type": "customField",
      "customField": "plugin::npistrapi.npistrapi"
    }
  }
}
