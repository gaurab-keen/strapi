{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Services",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "text",
      "pluginOptions": {},
      "required": true
    },
    "url": {
      "pluginOptions": {},
      "type": "text",
      "required": true,
      "unique": true
    },
    "power_by_service_plus": {
      "pluginOptions": {},
      "type": "boolean"
    },
    "url_is_on_gov_domain": {
      "pluginOptions": {},
      "type": "boolean",
      "required": true
    },
    "keywords": {
      "pluginOptions": {},
      "type": "text",
      "required": true
    },
    "description": {
      "pluginOptions": {},
      "type": "text",
      "required": true
    },
    "people_group": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::people-group.people-group",
      "inversedBy": "services"
    },
    "service_language": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::service-language.service-language",
      "inversedBy": "services"
    },
    "interaction_type": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::interaction-type.interaction-type",
      "inversedBy": "services"
    },
    "is_show_homepage": {
      "type": "boolean"
    },
    "homepage_img": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "review": {
      "type": "customField",
      "customField": "plugin::npistrapi.npistrapi"
    },
    "portal_link": {
      "type": "text"
    },
    "metadata_lang": {
      "type": "enumeration",
      "enum": [
        "Assamese",
        "Bengali",
        "Bodo",
        "Dogri",
        "English",
        "Gujrati",
        "Hindi",
        "Kannada",
        "Kashmiri",
        "Konkani",
        "Maithili",
        "Malayalam",
        "Marathi",
        "Meitei (Manipuri)",
        "Nepali",
        "Odia",
        "Punjabi",
        "Sanskrit",
        "Santali",
        "Sindhi",
        "Tamil",
        "Telugu",
        "Urdu"
      ],
      "default": "English"
    },
    "listing_page": {
      "type": "boolean"
    },
    "form_available": {
      "type": "boolean"
    },
    "login_required": {
      "type": "boolean"
    },
    "in_UMANG": {
      "type": "boolean"
    },
    "UMANG_url": {
      "type": "text"
    },
    "in_service_plus": {
      "type": "boolean"
    },
    "sp_url": {
      "type": "text"
    },
    "translation_status": {
      "type": "string"
    },
    "translation_id": {
      "type": "integer"
    },
    "service_plus_id": {
      "type": "integer"
    },
    "maturity": {
      "type": "enumeration",
      "enum": [
        "Partially Online",
        "Information",
        "Fully Online"
      ]
    },
    "owner": {
      "type": "enumeration",
      "enum": [
        "Central",
        "State"
      ]
    },
    "state_districts": {
      "type": "customField",
      "customField": "plugin::npistrapi.npistrapi2"
    }
  }
}
